<div id="page-wrapper">
	<div class="row">
		<div class="col-lg-12">
			<h1 class="page-header">Add new training</h1>
		</div>
	</div>
	<div class="col-lg-12">
		<div class="panel panel-default">
			<div class="panel-heading">
				Upload
			</div>
			<div class="panel-body">
				<div class="row">
          <div class="col-sm-6 col-md-4">
            <%= simple_form_for :track, url: upload_path, html: {multipart: true}  do |f| %>
                <%= f.input :name, placeholder: 'Name' %>
                <%= f.input :description, placeholder: 'Description' %>
                <%= f.input :gpx, as: :file %>
                <%= f.button :submit, class: 'btn btn-default btn-primary'  %>
            <% end %>
          </div>
        </div>
			</div>
		</div>
	</div>
</div>